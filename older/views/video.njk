{% extends 'layout.njk' %}

{% block content %}
    <section id="video">

        <div class="video" id="{{item.id}}">
            <div class="video-container">
                <h2>{{item.title}}</h2>
                <iframe src="https://www.youtube.com/embed/{{ item.id }}" frameborder="0"></iframe>
            </div>
        </div>

    </section>
{% endblock %}